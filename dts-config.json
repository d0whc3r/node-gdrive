{
  "compilationOptions": {
    "preferredConfigPath": "./tsconfig.json"
  },
  "entries": [
    {
      "filePath": "./src/index.ts",
      "outFile": "./dist/index.d.ts",
      "failOnClass": false,
      "libraries": {
        "allowedTypesLibraries": ["node", "gaxios"],
        "importedLibraries": ["googleapis"],
        "inlinedLibraries": []
      },
      "output": {
        "inlineDeclareGlobals": false,
        "sortNodes": true,
        "inlineDeclareExternals": true
      }
    }
  ]
}
