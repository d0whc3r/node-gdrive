{
  "compilationOptions": {
    "preferredConfigPath": "./tsconfig.json"
  },
  "entries": [
    {
      "filePath": "./src/index.ts",
      "outFile": "./dist/index.d.ts",
      "failOnClass": false,
      "libraries": {
        "allowedTypesLibraries": [
          "node"
        ],
        "importedLibraries": [
          "googleapis",
          "node",
          "gaxios"
        ],
        "inlinedLibraries": [
          "typescript",
          "gaxios"
        ]
      },
      "output": {
        "inlineDeclareGlobals": false,
        "sortNodes": true,
        "inlineDeclareExternals": true,
        "umdModuleName": "gdrive"
      }
    }
  ]
}
