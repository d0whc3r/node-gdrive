{
  "name": "@d0whc3r/node-gdrive",
  "version": "2.0.1",
  "license": "MIT",
  "author": {
    "email": "d0whc3r@gmail.com",
    "name": "d0whc3r",
    "url": "https://github.com/d0whc3r"
  },
  "bin": {
    "gdrive": "./bin/cli.js"
  },
  "repository": "https://github.com/d0whc3r/node-gdrive.git",
  "keywords": [
    "node",
    "google",
    "drive",
    "gdrive",
    "backup",
    "upload",
    "compress",
    "copy",
    "googleapi"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- node -r ts-node/register -r tsconfig-paths/register tests/index.ts",
    "cli:dev": "dotenv -e .env -- yarn cli",
    "cli": "node -r ts-node/register -r tsconfig-paths/register cli/cli.ts",
    "lint": "tslint -p .",
    "build:dist": "rimraf dist && webpack --config webpack/webpack.config.js",
    "build:cli": "rimraf bin && webpack --config webpack/webpack.config-cli.js",
    "build:dts": "dts-bundle-generator -o types/index.d.ts src/index.ts --umd-module-name=gdrive",
    "build": "yarn build:dist && yarn build:cli",
    "test": "jest -i --verbose --colors",
    "test:fakeci": "dotenv -e test.env -- yarn test",
    "test:coverage": "yarn test --coverage",
    "test:send-coverages": "yarn coverage:codecovs && yarn coverage:coveralls && yarn coverage:codacy",
    "coverage:codecovs": "codecov -f coverage/*.json -t ${CODECOVS_TOKEN}",
    "coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage:codacy": "cat ./coverage/lcov.info | codacy-coverage -p . --language typescript",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "archiver": "3.1.1",
    "google-auth-library": "5.2.1",
    "googleapis": "43.0.0",
    "mime-types": "2.1.24",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@types/archiver": "3.0.0",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/jest": "24.0.18",
    "@types/mime-types": "2.1.0",
    "@types/nock": "10.0.3",
    "@types/node": "12.7.5",
    "codacy-coverage": "3.4.0",
    "codecov": "3.6.1",
    "colors": "1.3.3",
    "command-line-args": "5.1.1",
    "command-line-usage": "6.0.2",
    "coveralls": "3.0.6",
    "dotenv-cli": "2.0.1",
    "dts-bundle-generator": "3.2.0",
    "glob": "7.1.4",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "jest-sonar-reporter": "2.0.0",
    "jest-transform-stub": "2.0.0",
    "mock-stdin": "0.3.1",
    "mysqldump": "3.1.0",
    "nock": "11.3.3",
    "rimraf": "3.0.0",
    "sonar-scanner": "3.1.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.1.0",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.20.0",
    "typescript": "3.6.3",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8",
    "webpack-merge": "4.2.2"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "main": "dist/gdrive.js",
  "module": "dist/es.js",
  "types": "types/index.d.ts",
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "jest-junit": {
    "outputDirectory": "./coverage",
    "outputName": "./junit.xml"
  }
}
