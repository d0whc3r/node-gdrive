{
  "name": "@d0whc3r/node-gdrive",
  "version": "3.0.1",
  "license": "MIT",
  "author": {
    "email": "d0whc3r@gmail.com",
    "name": "d0whc3r",
    "url": "https://github.com/d0whc3r"
  },
  "bin": {
    "gdrive": "./bin/cli.js"
  },
  "repository": "https://github.com/d0whc3r/node-gdrive.git",
  "keywords": [
    "node",
    "google",
    "drive",
    "gdrive",
    "backup",
    "upload",
    "compress",
    "copy",
    "googleapi"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- node -r ts-node/register -r tsconfig-paths/register tests/index.ts",
    "cli:dev": "dotenv -e .env -- yarn cli",
    "cli": "node -r ts-node/register -r tsconfig-paths/register cli/cli.ts",
    "lint": "tslint -p .",
    "build:dist": "rimraf dist bin && rollup -c",
    "build:dts": "dts-bundle-generator -o src/main.d.ts src/index.ts --umd-module-name=gdrive",
    "copy:dts": "cp src/main.d.ts dist/index.d.ts",
    "build": "yarn build:dist && yarn copy:dts",
    "test": "jest -i --verbose --colors",
    "test:fakeci": "dotenv -e test.env -- yarn test",
    "test:coverage": "yarn test --coverage",
    "test:send-coverages": "yarn coverage:codecovs && yarn coverage:coveralls && yarn coverage:codacy",
    "coverage:codecovs": "codecov -f coverage/*.json -t ${CODECOVS_TOKEN}",
    "coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage:codacy": "cat ./coverage/lcov.info | codacy-coverage -p . --language typescript",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "archiver": "3.1.1",
    "colors": "1.4.0",
    "command-line-args": "5.1.1",
    "command-line-usage": "6.1.0",
    "glob": "7.1.6",
    "googleapis": "45.0.0",
    "mime-types": "2.1.24",
    "moment": "2.24.0",
    "mysqldump": "3.1.0"
  },
  "devDependencies": {
    "google-auth-library": "5.5.1",
    "@babel/core": "7.6.4",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-regenerator": "7.4.5",
    "@babel/preset-env": "7.6.3",
    "@types/archiver": "3.0.0",
    "@types/command-line-args": "5.0.0",
    "@types/command-line-usage": "5.0.1",
    "@types/jest": "24.0.22",
    "@types/mime-types": "2.1.0",
    "@types/nock": "11.1.0",
    "@types/node": "12.12.7",
    "builtin-modules": "3.1.0",
    "codacy-coverage": "3.4.0",
    "codecov": "3.6.1",
    "core-js": "3.3.6",
    "coveralls": "3.0.7",
    "dotenv-cli": "3.0.0",
    "dts-bundle-generator": "3.3.0",
    "jest": "24.9.0",
    "jest-junit": "9.0.0",
    "jest-sonar-reporter": "2.0.0",
    "jest-transform-stub": "2.0.0",
    "mock-stdin": "0.3.1",
    "nock": "11.7.0",
    "regenerator-runtime": "0.13.3",
    "rimraf": "3.0.0",
    "rollup": "1.26.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.2",
    "rollup-plugin-typescript2": "0.25.2",
    "sonar-scanner": "3.1.0",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "files": [
    "dist",
    "bin"
  ],
  "main": "dist/gdrive.cjs.js",
  "module": "dist/gdrive.es.mjs",
  "types": "dist/index.d.ts",
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml"
  },
  "jest-junit": {
    "outputDirectory": "./coverage",
    "outputName": "./junit.xml"
  }
}
